{
  "name": "smartfyt-api",
  "version": "1.0.0",
  "description": "SmartFyt API - Backend service for student-athlete performance tracking",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:routes": "vitest run src/tests/routes/",
    "test:user-management": "vitest run src/tests/routes/userManagement.test.ts",
    "test:journals": "vitest run src/tests/routes/journals.test.ts",
    "test:teams": "vitest run src/tests/routes/teams.test.ts",
    "test:quests": "vitest run src/tests/routes/questManagement.test.ts",
    "test:leaderboard": "vitest run src/tests/routes/leaderboard.test.ts",
    "test:dashboard": "vitest run src/tests/routes/dashboard.test.ts",
    "test:upload": "vitest run src/tests/routes/upload.test.ts",
    "test:challenge-progress": "vitest run src/tests/routes/challengeProgress.test.ts",
    "test:new-features": "vitest run src/tests/routes/upload.test.ts src/tests/routes/challengeProgress.test.ts",
    "test:verify-mocking": "node scripts/verify-mocking.js",
    "lint": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.682.0",
    "@aws-sdk/client-sqs": "^3.682.0",
    "@aws-sdk/s3-request-presigner": "^3.691.0",
    "@fastify/cors": "^8.5.0",
    "@kinde/jwt-validator": "^0.4.0",
    "@prisma/client": "^6.4.1",
    "dotenv": "^17.2.0",
    "fastify": "^4.27.1",
    "pg": "^8.13.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "redis": "^4.7.0",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.13.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.32.0",
    "prisma": "^6.4.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "api",
    "fastify",
    "prisma",
    "typescript",
    "student-athlete",
    "performance-tracking"
  ],
  "author": "SmartFyt Team",
  "license": "MIT"
}
